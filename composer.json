{
    "name": "luan098/mini_4.0",
    "description": "Fork mini 3 with improvements",
    "type": "project",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Mini\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.11"
        },
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require": {
        "robmorgan/phinx": "^0.13.4",
        "intervention/image": "^2.7",
        "symfony/routing": "^6.3",
        "symfony/mailer": "^6.3",
        "symfony/http-foundation": "^6.3"
    },
    "scripts": {
        "phinx": "php vendor/bin/phinx",
        "migrate": "php vendor/bin/phinx migrate",
        "m:create": "php vendor/bin/phinx create",
        "m:rollback": "php vendor/bin/phinx rollback"
    }
}
